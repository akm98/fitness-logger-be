{
  "name": "node_boilerplate",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "dist": "npx tsc",
    "dev": "cross-env NODE_ENV=development nodemon --legacy-watch src/server.ts",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "debug": "node --inspect-brk -r ts-node/register src/index.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.3",
    "@types/node": "^22.13.14",
    "@types/source-map-support": "^0.5.10",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^4.3.8",
    "lint-staged": "^15.5.0",
    "nodemon": "^3.1.9",
    "prettier": "3.5.3",
    "prettier-eslint": "^16.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "colorette": "^2.0.20",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.1",
    "zod": "^3.25.74"
  }
}
